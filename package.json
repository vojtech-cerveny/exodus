{
  "name": "exodus-90",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "showDuplicates": "jscpd --pattern '/src/**/**' --ignore='**/*.md'",
    "prepare": "husky install",
    "release": "standard-version",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "generateFiles": "npx ts-node src/app/(app)/data/text-to-md.ts",
    "test": "jest",
    "payload": "PAYLOAD_CONFIG_PATH=src/payload.config.ts payload",
    "migrate": "payload migrate && prisma migrate deploy",
    "migrate:prod": "NODE_ENV=production ts-node scripts/safe-migrate.ts",
    "migrate:ci": "CI=true NODE_ENV=production ts-node scripts/safe-migrate.ts",
    "migrate:dry-run": "npx payload migrate:status && prisma migrate status",
    "db:restore": "ts-node scripts/restore-db.ts",
    "db:backup": "ts-node scripts/backup-db.ts",
    "db:backup:custom": "ts-node scripts/backup-db.ts --format custom",
    "db:backup:plain": "ts-node scripts/backup-db.ts --format plain"
  },
  "type": "module",
